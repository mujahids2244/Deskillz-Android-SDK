<ScrollView
    android:layout_marginTop="10dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_margin="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/edit_text_deisgn"
            android:layout_marginTop="10dp"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_10dp"
                android:layout_marginStart="@dimen/size_20dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/size_30dp"
                    android:layout_height="@dimen/size_30dp"
                    android:src="@drawable/starlarge" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/size_10dp"
                    android:text="Practice League"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/size_10dp"
                android:layout_marginStart="@dimen/size_20dp"
                android:layout_marginEnd="@dimen/size_20dp">

                <RelativeLayout
                    android:id="@+id/deskillzLevelLayout"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/size_150dp"
                    android:layout_marginEnd="@dimen/size_5dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:padding="@dimen/size_5dp">

                    <ImageView
                        android:layout_width="@dimen/size_30dp"
                        android:layout_height="@dimen/size_50dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/size_30dp"
                        android:src="@drawable/d_level_large" />

                    <ProgressBar
                        android:id="@+id/progressBar"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="@dimen/size_120dp"
                        android:layout_height="@dimen/size_100dp"
                        android:layout_centerHorizontal="true"
                        android:max="100"
                        android:progress="40"
                        android:progressDrawable="@drawable/progress_design" />

                    <RelativeLayout
                        android:layout_width="@dimen/size_30dp"
                        android:layout_height="@dimen/size_30dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/size_75dp"
                        android:background="@drawable/background_layout_red_cricle"
                        android:backgroundTint="#D52116">

                        <TextView
                            android:id="@+id/progressXp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:fontFamily="@font/titan_one_regular"
                            android:text="0"
                            android:textColor="@color/white" />

                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:text="Deskillz Level"
                        android:textStyle="bold" />

                </RelativeLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@color/greyish"/>

                <RelativeLayout
                    android:id="@+id/battleFieldLayout"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/size_150dp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:padding="@dimen/size_5dp">

                    <ImageView
                        android:layout_width="@dimen/size_30dp"
                        android:layout_height="@dimen/size_50dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/size_30dp"
                        android:src="@drawable/bflarge" />

                    <ProgressBar
                        android:id="@+id/progressBar2"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="@dimen/size_120dp"
                        android:layout_height="@dimen/size_100dp"
                        android:layout_centerHorizontal="true"
                        android:max="100"
                        android:progress="40"
                        android:progressDrawable="@drawable/progress_design"
                        android:progressTint="#51FCDF" />

                    <RelativeLayout
                        android:layout_width="@dimen/size_30dp"
                        android:layout_height="@dimen/size_30dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/size_75dp"
                        android:background="@drawable/background_layout_red_cricle"
                        android:backgroundTint="#51FCDF">

                        <TextView
                            android:id="@+id/progressXp2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:fontFamily="@font/titan_one_regular"
                            android:text="0"
                            android:textColor="@color/black" />
                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:text="Deskillz Level"
                        android:textStyle="bold" />

                </RelativeLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@color/greyish"/>

                <RelativeLayout
                    android:id="@+id/bronzeTiersLayout"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/size_150dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:padding="@dimen/size_5dp">

                    <ImageView
                        android:layout_width="@dimen/size_30dp"
                        android:layout_height="@dimen/size_50dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/size_30dp"
                        android:src="@drawable/ticket_with_bg" />

                    <ProgressBar
                        android:id="@+id/progressBar3"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="@dimen/size_120dp"
                        android:layout_height="@dimen/size_100dp"
                        android:layout_centerHorizontal="true"
                        android:max="100"
                        android:progress="40"
                        android:progressDrawable="@drawable/progress_design"
                        android:progressTint="#A73C11" />

                    <RelativeLayout
                        android:layout_width="@dimen/size_30dp"
                        android:layout_height="@dimen/size_30dp"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/size_75dp"
                        android:background="@drawable/background_layout_red_cricle"
                        android:backgroundTint="#A73C11">

                        <TextView
                            android:id="@+id/progressXp3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:fontFamily="@font/titan_one_regular"
                            android:text="0"
                            android:textColor="@color/white" />

                    </RelativeLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:text="Deskillz Level"
                        android:textStyle="bold" />

                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/edit_text_deisgn"
            android:layout_marginTop="10dp"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_10dp"
                android:layout_marginStart="@dimen/size_20dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/size_30dp"
                    android:layout_height="@dimen/size_30dp"
                    android:src="@drawable/starlarge" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/size_10dp"
                    android:text="Trophies Earned"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="@dimen/size_15dp"

                android:background="@drawable/background_layout_white_rounded"

                >
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recycleListTrophies"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_margin="@dimen/size_10dp"
                android:layout_height="wrap_content"
                tools:listitem="@layout/adapter_trophies_view"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                />

                <androidx.core.widget.ContentLoadingProgressBar
                    android:id="@+id/progressBarOne"
                    style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/size_20dp"
                    android:layout_centerHorizontal="true"
                    android:scaleY="2"
                    android:layout_marginStart="@dimen/size_10dp"
                    android:layout_marginTop="@dimen/size_20dp"
                    android:layout_marginEnd="@dimen/size_65dp"
                    android:max="100"
                    android:progress="20"
                    android:progressTint="#E33960"
                    />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/edit_text_deisgn"
            android:layout_marginTop="10dp"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_10dp"
                android:layout_marginStart="@dimen/size_20dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/size_30dp"
                    android:layout_height="@dimen/size_30dp"
                    android:src="@drawable/starlarge" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/size_10dp"
                    android:text="WIN/WIN STREAK"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/size_10dp"
                android:layout_marginStart="@dimen/size_20dp"
                android:layout_marginEnd="@dimen/size_20dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/matchesWon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="N/a"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="Matches Won"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp" />




                </LinearLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="20dp"
                    android:background="@color/greyish"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:layout_marginStart="@dimen/size_20dp"
                    android:orientation="vertical">



                    <TextView
                        android:id="@+id/winStreak"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="N/a"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="Win Streak"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:layout_marginBottom="20dp"
                        android:textSize="@dimen/text_size_18sp" />






                </LinearLayout>



            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginBottom="@dimen/size_10dp"
            android:background="@drawable/edit_text_deisgn"
            android:layout_marginTop="10dp"
            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/size_10dp"
                android:layout_marginStart="@dimen/size_20dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/size_30dp"
                    android:layout_height="@dimen/size_30dp"
                    android:src="@drawable/starlarge" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/size_10dp"
                    android:text="You Vs EveryOne"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text_size_18sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/size_10dp"
                android:layout_marginStart="@dimen/size_20dp"
                android:layout_marginEnd="@dimen/size_20dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/win"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="N/a"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="W"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp" />




                </LinearLayout>

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="20dp"
                    android:background="@color/greyish"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:layout_marginStart="@dimen/size_20dp"
                    android:orientation="vertical">



                    <TextView
                        android:id="@+id/lose"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="N/a"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:text="L"
                        android:fontFamily="@font/titan_one_regular"
                        android:textColor="@color/white"
                        android:layout_marginBottom="20dp"
                        android:textSize="@dimen/text_size_18sp" />






                </LinearLayout>



            </LinearLayout>

        </LinearLayout>

    </LinearLayout>



</ScrollView>






</LinearLayout>