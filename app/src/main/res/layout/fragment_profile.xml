<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:padding="@dimen/size_10dp"
    android:background="@drawable/splash_bg"
    tools:context=".ui.fragment.navBarFragments.ProfileFragment">


    <LinearLayout
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="10dp">

        <ImageView
            android:id="@+id/backtoDashboard"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:src="@drawable/ic_baseline_keyboard_arrow_left_24" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="3"
            android:text="Profile"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="@dimen/_17sdp" />


      <View
          android:layout_width="0dp"
          android:layout_height="0dp"
          android:layout_weight="2"/>


    </LinearLayout>


    <RelativeLayout
        android:id="@+id/practiceLeague1"
        android:layout_width="match_parent"
        android:layout_height="165dp">

        <com.arhamsoft.deskilz.ui.backGroundLayout.BackGroundLayoutView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha=".8"
            app:backgroundColorView="@color/gray_bg_color"
            app:cornersValue="@dimen/size_10dp"
            app:corners_side="1"
            app:lower_cut_value="@dimen/size_20dp"
            app:upper_cut_value="@dimen/size_30dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_margin="@dimen/size_10dp"

                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/userImg"
                    android:layout_width="@dimen/size_80dp"
                    android:layout_height="@dimen/size_80dp"
                    android:background="@drawable/ic_img_layout"
                    android:src="@drawable/ic_baseline_person_24"

                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingStart="10dp"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:id="@+id/userName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Name"
                        android:textSize="@dimen/text_size_25sp"
                        android:textColor="@color/white"
                        android:fontFamily="@font/titan_one_regular"/>

                    <ImageView
                        android:id="@+id/Flag"
                        android:layout_width="40dp"
                        android:layout_height="30dp"
                        android:background="@color/white"
                        android:layout_marginTop="5dp"
                        />

                </LinearLayout>



            </LinearLayout>

            <TextView
                android:id="@+id/shoutout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/blue1"
                android:text="shoutout"
                android:textColor="@color/white"
                android:maxLines="1"
                android:paddingStart="@dimen/size_5dp"
                />


            <RelativeLayout
                android:id="@+id/status_bar"
                android:layout_width="match_parent"
                android:layout_height="@dimen/size_65dp"
                android:layout_below="@+id/topBar"
                android:layout_centerHorizontal="true"
                android:orientation="horizontal"
                >

                <com.arhamsoft.deskilz.ui.backGroundLayout.BackGroundLayoutView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/size_65dp"
                    android:alpha=".8"
                    app:apply_gradient="1"
                    app:cornersValue="@dimen/size_10dp"
                    app:corners_side="1"
                    app:gradient1="@color/red1"
                    app:gradient2="@color/red2"
                    app:lower_cut_value="@dimen/size_20dp"
                    app:upper_cut_value="@dimen/size_2dp" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="horizontal">


<!--                <LinearLayout-->
<!--                    android:id="@+id/ticketLayout"-->
<!--                    android:layout_width="wrap_content"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:layout_centerVertical="true"-->
<!--                    android:gravity="center"-->
<!--                    android:layout_marginStart="@dimen/size_20dp"-->
<!--                    android:orientation="horizontal">-->

<!--                    <ImageView-->
<!--                        android:layout_width="@dimen/size_30dp"-->
<!--                        android:layout_height="@dimen/size_30dp"-->
<!--                        android:src="@drawable/ticket_without_bg" />-->

<!--                    <TextView-->
<!--                        android:id="@+id/ticketsNo"-->
<!--                        android:layout_width="wrap_content"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        android:layout_marginStart="@dimen/size_5dp"-->
<!--                        android:text="N/a"-->
<!--                        android:fontFamily="@font/titan_one_regular"-->
<!--                        android:textColor="@color/white"-->
<!--                        android:textSize="@dimen/text_size_18sp" />-->

<!--                </LinearLayout>-->

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/size_30dp"
                        android:layout_height="@dimen/size_30dp"
                        android:src="@drawable/coin_medium" />

                    <TextView
                        android:id="@+id/coinNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:fontFamily="@font/titan_one_regular"
                        android:text="N/a"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginEnd="@dimen/size_20dp"
                    android:layout_weight="1"

                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="$"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_20sp"
                        android:fontFamily="@font/titan_one_regular"/>

                    <TextView
                        android:id="@+id/dollar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/size_5dp"
                        android:fontFamily="@font/titan_one_regular"

                        android:text="N/a"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_18sp"/>

                </LinearLayout>

                </LinearLayout>


            </RelativeLayout>



        </LinearLayout>




    </RelativeLayout>



<!--    <TextView-->
<!--        android:id="@+id/shoutout"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:textColor="@color/white"-->
<!--        android:textSize="20sp"-->
<!--        android:paddingStart="5dp"-->
